{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/app.js"
        },
        {
            "src": "/listings/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/listings/:id/reviews/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/users/(.*)",
            "dest": "/app.js"
        }
        
    ]
}
